<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Model xmlns="http://webservice.datawave/v1" name="DATAWAVE_EDGE">
    <!-- 
         NOTE:
        
         Unlike event-based query models, here the 'fieldName' (aka 'nameOnDisk') values for the edge query model 
         actually map to field names defined within EdgeModelContext.xml. Since edge names don't really exist on 
         disk in the way that event field names do, we have the flexibility to vary Datawave's internal field 
         name representation as needed for the deployment environment's default query syntax.
    -->
    <Mappings>
        <Mapping fieldName="SOURCE" modelFieldName="VERTEXA" direction="REVERSE" indexOnly="false"/>
        <Mapping fieldName="SOURCE" modelFieldName="VERTEXA" direction="FORWARD" indexOnly="false"/>
        
        <Mapping fieldName="SINK" modelFieldName="VERTEXB" direction="REVERSE" indexOnly="false"/>
        <Mapping fieldName="SINK" modelFieldName="VERTEXB" direction="FORWARD" indexOnly="false"/>
        
        <Mapping fieldName="RELATION" modelFieldName="RELATION" direction="REVERSE" indexOnly="false"/>
        <Mapping fieldName="RELATION" modelFieldName="RELATION" direction="FORWARD" indexOnly="false"/>
        
        <Mapping fieldName="TYPE" modelFieldName="TYPE" direction="REVERSE" indexOnly="false"/>
        <Mapping fieldName="TYPE" modelFieldName="TYPE" direction="FORWARD" indexOnly="false"/>
        
        <Mapping fieldName="ATTRIBUTE1" modelFieldName="ATTR1" direction="REVERSE" indexOnly="false"/>
        <Mapping fieldName="ATTRIBUTE1" modelFieldName="ATTR1" direction="FORWARD" indexOnly="false"/>        
        
        <Mapping fieldName="ATTRIBUTE2" modelFieldName="ATTR2" direction="REVERSE" indexOnly="false"/>
        <Mapping fieldName="ATTRIBUTE2" modelFieldName="ATTR2" direction="FORWARD" indexOnly="false"/>
                
        <Mapping fieldName="ATTRIBUTE3" modelFieldName="ATTR3" direction="REVERSE" indexOnly="false"/>
        <Mapping fieldName="ATTRIBUTE3" modelFieldName="ATTR3" direction="FORWARD" indexOnly="false"/>       
            
    </Mappings>
</Model>

